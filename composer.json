{
    "name": "shopengine/nova",
    "description": "Shopengine Nova Integration",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^3.10",
        "laravel/framework": "^7.0|^8.0|^9.0",
        "shopengine/client-php": "^2.2.0",
        "optimistdigital/nova-translations-loader": "^3.1.4"
    },
    "autoload": {
        "psr-4": {
            "ShopEngine\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShopEngine\\Nova\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
