{
    "name": "shopengine/nova",
    "description": "Shopengine Nova Integration",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "shopengine/client-php": "3.0.1",
        "brainspin/laravel-klicktipp": "1.0.0",
        "optimistdigital/nova-translations-loader": "^3.1.2"
    },
    "autoload": {
        "psr-4": {
            "ShopEngine\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShopEngine\\Nova\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
