{
    "name": "brainspin/shopengine-nova",
    "description": "Shopengine Nova Integration",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "brainspin/client-php": "3.0.0",
        "optimistdigital/nova-translations-loader": "*"
    },
    "autoload": {
        "psr-4": {
            "Brainspin\\Novashopengine\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brainspin\\Novashopengine\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
